<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>8.0</LangVersion>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>UniWalker</PackageId>
    <Version>1.0</Version>
    <Authors>eugenie32b</Authors>
    <Company>private</Company>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/eugenie32b/uniwalker</RepositoryUrl>
    <PackageTags>UniWalker;dynamic;config</PackageTags>
    <Description>UniWalker is a library that allows you to navigate files in XML/JSON/INI formats using abilities of dynamic variables.
    For more information see at https://github.com/eugenie32b/uniwalker
    </Description>

  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Text.Json" Version="4.7.2" />
  </ItemGroup>

</Project>
